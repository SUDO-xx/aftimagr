<h1>New <%= category_singular_name %></h1>

<%% form_for(@<%= category_singular_name %>) do |f| %>
  <%%= f.error_messages %>

<% for attribute in attributes -%>
  <p>
    <%%= f.label :<%= attribute.name %> %><br />
    <%%= f.<%= attribute.field_type %> :<%= attribute.name %> %>
  </p>
<% end -%>
  <p>
    <%%= f.submit "Create" %>
  </p>
<%% end %>

<%%= link_to 'Back', <%= categories_plural_name %>_path %>
